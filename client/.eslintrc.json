{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": true
  },
  "rules": {"import/prefer-default-export": false},
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
